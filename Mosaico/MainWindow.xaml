<Window x:Class="Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="660" Width="700" MinHeight="500" Icon="./assets/128.ico" MinWidth="450">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Background="#FF7710F3" Height="100"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Background="#FF7710F3" Width="100"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="#FF5A9E09"></TextBlock>
       
        <TextBlock DockPanel.Dock="Right" Background="#FF5A9E09" Width="100"></TextBlock>
        <DockPanel>

            
            <TextBlock DockPanel.Dock="Bottom" Height="100" Background="#FFF10808"></TextBlock>
            <TextBlock DockPanel.Dock="Left" Background="#FFF10808" Width="100"></TextBlock>
            <TextBlock DockPanel.Dock="Top" Background="#FFCA10F3" Height="100"></TextBlock>
            <TextBlock DockPanel.Dock="Right" Background="#FFCA10F3" Width="100"></TextBlock>
            <TextBlock  Background="#FF191819" ></TextBlock>
        </DockPanel>
        
    </DockPanel>
</Window>
